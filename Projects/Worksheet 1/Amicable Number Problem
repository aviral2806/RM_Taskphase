#include <iostream>
using namespace std;
int main(){
    int n1=0;
    int n2=0;
    cout << "Enter first number: ";
    cin >> n1;
    cout << endl << "Enter second number: ";
    cin >> n2;
    int sum1=0;
    int sum2=0;
    for(int i=1;i<=(n1/2);i++){
        if(n1%i==0){
            sum1+=i;
        }
    }
    
     for(int i=1;i<=(n2/2);i++){
        if(n2%i==0){
            sum2+=i;
        }
    }
    
    if(sum1==n2 && sum2==n1){
        cout << endl << "They are amicable numbers" << endl;
    }
    
    else{
        cout << endl << "They are not amicable numbers" << endl;
    }
}
